= render partial: 'layouts/admin/header'
= render partial: 'layouts/admin/left_menu'

.content_wrapper
  = render partial: 'layouts/admin/left_detail_menu' 
  .data_content
    %ul.paths
      %li= link_to 'Partners', admin_partners_path
      %li= link_to 'Add new', new_admin_partner_path
    .h3.marginbottom-20= t("view.admin.partner.new_partner")

    = form_for Partner.new, :url => { :controller => @entity_table, :action => 'create' }, :method => :post do |f|

      = render partial: "admin/#{@entity_table}/form_data", :locals => { :f => f, :entity => Partner.new }

      .clearfix
      .margintop-30
        .message_errors
          %ul
        .loading_button
          %img{:src => '/assets/admin/loading_button.gif'}
        = link_to 'save', '#', :class => 'default_button dark create_entity'